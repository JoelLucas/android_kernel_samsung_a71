/* r06 */ 
&i2c_13 {
	s2mu106@3D {
		status = "okay";
		compatible = "samsung,s2mu106mfd";
		reg = <0x3D>;
		pinctrl-names = "default";
		pinctrl-0 = <&if_pmic_irq>;
		s2mu106,irq-gpio = <&tlmm 68 0>;
		s2mu106,wakeup;
	};
};

&i2c_14 {
	usbpd-s2mu106@3C {
		compatible = "s2mu106-usbpd";
		status = "okay";
		reg = <0x3C>;
		pinctrl-names = "default";
		pinctrl-0     = <&usbpd_irq>;
		usbpd,usbpd_int = <&tlmm 32 0>;
	};
};